{
  "name": "@matejmazur/react-katex",
  "version": "1.0.4",
  "description": "Display math in TeX with KaTeX and ReactJS",
  "keywords": [
    "react",
    "tex",
    "latex",
    "math",
    "katex"
  ],
  "homepage": "https://github.com/MatejMazur/react-katex",
  "main": "dist/react-katex.js",
  "umd:main": "dist/react-katex.umd.js",
  "module": "dist/react-katex.mjs",
  "source": "index.js",
  "author": {
    "name": "Matej Mazur",
    "email": "matej.mazur@mail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MatejMazur/react-katex.git"
  },
  "bugs": {
    "url": "https://github.com/MatejMazur/react-katex/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "cd demo && npm start",
    "build": "microbundle --jsx React.createElement",
    "dev": "microbundle watch",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{index,index.spec,demo/index}.js": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "katex": "^0.10.0",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.8.0",
    "eslint-plugin-jest": "^21.27.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "microbundle": "^0.7.0",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-testing-library": "^5.2.3",
    "snapshot-diff": "^0.4.0"
  },
  "peerDependencies": {
    "react": ">=16.6.0"
  }
}
